   "source": [
    "# Pivot table: unique patients per product by year\n",
    "unique_patients_df = data_filtered.pivot_table(\n",
    "    index=data_filtered['claim_date'].dt.year,\n",
    "    columns='product_name',\n",
    "    values='Patient_id',\n",
    "    aggfunc=pd.Series.nunique,\n",
    "    fill_value=0\n",
    ")\n",
    "\n",
    "# Show the result\n",
    "unique_patients_df"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 178,
   "id": "2193abbb-08ab-47df-968d-a113c472e34f",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 1200x800 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Mapping product codes to product names\n",
    "product_code_to_name = {\n",
    "    \"J1885\": \"Ketotrom\",\n",
    "    \"J2250\": \"Midoride\",\n",
    "    \"J3010\": \"Fentirate\",\n",
    "    \"J2704\": \"Profativ\"\n",
    "}\n",
    "main_data['product_name'] = main_data['product_code'].map(product_code_to_name)\n",
    "\n",
    "# Custom colors for consistency\n",
    "colors = {\n",
    "    'Fentirate': '#4C72B0',\n",
    "    'Ketotrom': '#DD8452',\n",
    "    'Midoride': '#55A868',\n",
    "    'Profativ': '#C44E52'\n",
    "}\n",
    "\n",
    "# Group data by year and product name, then count the number of unique patients\n",
    "patients_per_year_product = data_filtered.groupby(\n",
    "    [data_filtered['claim_date'].dt.year, 'product_name']\n",
    ")['Patient_id'].nunique().unstack(fill_value=0)\n",
    "\n",
    "# Plotting the stacked bar graph with custom colors\n",
    "plt.figure(figsize=(12, 8))\n",
    "ax = patients_per_year_product.plot(\n",
    "    kind='bar',\n",
    "    stacked=True,\n",
    "    ax=plt.gca(),\n",
    "    color=[colors[col] for col in patients_per_year_product.columns]\n",
    ")\n",
    "\n",
    "plt.title('Number of Patients of Each Product Per Year', fontsize=35)\n",
    "plt.xlabel('Year', fontsize=25)\n",
    "plt.ylabel('Number of Patients', fontsize=25)\n",
    "plt.xticks(rotation=0, fontsize=25)\n",
    "plt.yticks(fontsize=25)\n",
    "plt.legend(title='Product', bbox_to_anchor=(1.05, 1), loc='upper left')\n",
    "\n",
    "# Annotate numbers on bars\n",
    "for bar in ax.containers:\n",
    "    ax.bar_label(bar, label_type='center', fontsize=25)\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 179,
   "id": "ced10f8e-6ddb-4590-aa67-7be280ccb2e8",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
