    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "\n",
    "# Map diagnosis code initials to specialties using what's already in main_data\n",
    "# Assumes 'Diagnosis code Speciality' and 'diagnosis_code_initials' already exist in main_data\n",
    "main_data['Diagnosis Specialty'] = main_data['Diagnosis code Speciality'] + ' (' + main_data['diagnosis_code_initials'] + ')'\n",
    "\n",
    "# Filter data for years 2016–2018\n",
    "main_data['claim_date'] = pd.to_datetime(main_data['claim_date'])  # ensure datetime type\n",
    "data_filtered = main_data[main_data['claim_date'].dt.year.isin([2016, 2017, 2018])]\n",
    "\n",
    "# --- PIE CHART for Top 5 Diagnosis Specialties ---\n",
    "top_5_diagnosis = data_filtered.groupby('Diagnosis Specialty')['claim_id'].count().nlargest(5)\n",
    "\n",
    "plt.figure(figsize=(10, 6))\n",
    "colors = sns.color_palette(\"pastel\")  # use a clean color palette\n",
    "\n",
    "pie_chart = top_5_diagnosis.plot(\n",
    "    kind='pie',\n",
    "    autopct='%1.2f%%',\n",
    "    startangle=140,\n",
    "    fontsize=12,\n",
    "    colors=colors\n",
    ")\n",
    "\n",
    "plt.title('Claims of Top 5 Diagnosis Specialties', fontsize=22)\n",
    "plt.ylabel('')  # remove y-axis label\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "# --- BAR CHART for HCP Specialty Groups ---\n",
    "hcp_specialty_counts = data_filtered.groupby('HCP Speciality')['npi_id'].nunique().sort_values(ascending=False).head(10)\n",
    "\n",
    "plt.figure(figsize=(12, 8))\n",
    "ax = sns.barplot(x=hcp_specialty_counts.values, y=hcp_specialty_counts.index, hue=hcp_specialty_counts.index, dodge=False, palette='viridis', legend=False)\n",
    "\n",
    "for p in ax.patches:\n",
    "    ax.annotate(\n",
    "        format(p.get_width(), '.0f'),\n",
    "        (p.get_width(), p.get_y() + p.get_height() / 2.),\n",
    "        ha='left',\n",
    "        va='center',\n",
    "        xytext=(4, 0),\n",
    "        textcoords='offset points',\n",
    "        fontsize=14\n",
    "    )\n",
