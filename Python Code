   "source": [
    "# Step 1: Drop duplicates to find new writers\n",
    "new_writers = joined_df_2016_2018.drop_duplicates(subset=['npi_id', 'product_code']).pivot_table(\n",
    "    index='Claim_year',\n",
    "    columns='product_code',\n",
    "    values='npi_id',\n",
    "    aggfunc=pd.Series.nunique,\n",
    "    fill_value=0\n",
    ")[procedure_groups.keys()]\n",
    "\n",
    "# Step 2: Calculate total writers\n",
    "total_writers = joined_df_2016_2018.pivot_table(\n",
    "    index='Claim_year',\n",
    "    columns='product_code',\n",
    "    values='npi_id',\n",
    "    aggfunc=pd.Series.nunique,\n",
    "    fill_value=0\n",
    ")[procedure_groups.keys()]\n",
    "\n",
    "# Step 3: Subtract to get continuing writers\n",
    "continuing_writers = total_writers - new_writers\n",
    "\n",
    "# Step 4: Display\n",
    "continuing_writers"
   ]
