    "# Extract claim year from claim_date\n",
    "data_filtered.loc[:, 'year'] = data_filtered['claim_date'].dt.year\n",
    "\n",
    "# Calculate average number of patients per HCP for each product per year\n",
    "patients_per_hcp = (\n",
    "    data_filtered.groupby(['product_name', 'year'])['Patient_id'].nunique() /\n",
    "    data_filtered.groupby(['product_name', 'year'])['npi_id'].nunique()\n",
    ")\n",
    "\n",
    "# Unstack for easier viewing or plotting\n",
    "patients_per_hcp_unstacked = patients_per_hcp.unstack(level=0)[['Ketotrom', 'Midoride', 'Fentirate', 'Profativ']]\n",
    "\n",
    "# Display the result\n",
    "patients_per_hcp_unstacked"
   ]
