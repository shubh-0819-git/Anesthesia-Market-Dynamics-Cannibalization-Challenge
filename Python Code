    "# Add product_name column\n",
    "product_code_to_name = {\n",
    "    \"J1885\": \"Ketotrom\",\n",
    "    \"J2250\": \"Midoride\",\n",
    "    \"J3010\": \"Fentirate\",\n",
    "    \"J2704\": \"Profativ\"\n",
