   "source": [
    "# Total Prescribing HCPs by Product and Year\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "\n",
    "# Step 1: Prepare data and add Year\n",
    "main_data['Year'] = pd.to_datetime(main_data['claim_date']).dt.year\n",
    "\n",
    "# Step 2: Filter relevant products and years\n",
    "procedure_groups = {\n",
    "    'J1885': 'Ketotrom',\n",
    "    'J2250': 'Midoride',\n",
    "    'J3010': 'Fentirate',\n",
    "    'J2704': 'Profativ'\n",
    "}\n",
    "\n",
    "filtered_df = main_data[\n",
    "    (main_data['Year'].isin([2016, 2017, 2018])) &\n",
    "    (main_data['product_code'].isin(procedure_groups.keys()))\n",
    "].copy()\n",
    "\n",
    "filtered_df['Product'] = filtered_df['product_code'].map(procedure_groups)\n",
    "\n",
    "# Step 3: Count unique HCPs per product and year\n",
    "hcp_counts = (\n",
    "    filtered_df\n",
    "    .groupby(['Year', 'Product'])['npi_id']\n",
    "    .nunique()\n",
    "    .reset_index(name='Unique_HCPs')\n",
    ")\n",
    "\n",
    "# Step 4: Plot\n",
    "product_colors = {\n",
    "    'Fentirate': '#1f77b4',\n",
    "    'Ketotrom': '#ff7f0e',\n",
    "    'Midoride': '#2ca02c',\n",
    "    'Profativ': '#d62728'\n",
    "}\n",
    "product_order = ['Fentirate', 'Ketotrom', 'Midoride', 'Profativ']\n",
    "\n",
    "plt.figure(figsize=(10, 6))\n",
    "sns.barplot(\n",
    "    data=hcp_counts,\n",
    "    x='Year',\n",
    "    y='Unique_HCPs',\n",
    "    hue='Product',\n",
    "    hue_order=product_order,\n",
    "    palette=product_colors\n",
    ")\n",
    "\n",
    "# Add bar labels\n",
    "for container in plt.gca().containers:\n",
    "    plt.bar_label(container, fmt='%d', padding=3, fontsize=10)\n",
    "\n",
    "# Format plot\n",
    "plt.title('Number of Unique HCPs Prescribing Each Product by Year', fontsize=16, pad=20)\n",
    "plt.xlabel('Claim Year', fontsize=14)\n",
    "plt.ylabel('Number of Unique HCPs', fontsize=14)\n",
    "plt.xticks(fontsize=12)\n",
    "plt.yticks(fontsize=12)\n",
    "plt.legend(title='Product', bbox_to_anchor=(1.02, 1), loc='upper left')\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
  },
