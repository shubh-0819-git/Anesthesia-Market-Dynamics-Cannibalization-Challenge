   "source": [
    "import pandas as pd\n",
    "\n",
    "# Step 1: Map procedure codes to product brand names\n",
    "procedure_groups = {\n",
    "    'J1885': 'Ketotrom',\n",
    "    'J2250': 'Midoride',\n",
    "    'J3010': 'Fentirate',\n",
    "    'J2704': 'Profativ'\n",
    "}\n",
    "\n",
    "# Step 2: Filter main_data for years 2016–2018 and copy\n",
    "main_data['claim_date'] = pd.to_datetime(main_data['claim_date'])\n",
    "joined_df_2016_2018 = main_data[main_data['claim_date'].dt.year.isin([2016, 2017, 2018])].copy()\n",
    "\n",
    "# Step 3: Add brand and year columns\n",
    "joined_df_2016_2018['Product_brand'] = joined_df_2016_2018['product_code'].map(procedure_groups)\n",
    "joined_df_2016_2018['Claim_year'] = joined_df_2016_2018['claim_date'].dt.year\n",
    "\n",
    "# Step 4: Create pivot table of new HCP writers by brand and year\n",
    "new_writers = joined_df_2016_2018.drop_duplicates(subset=['npi_id', 'Product_brand']).pivot_table(\n",
    "    index='Claim_year',\n",
    "    columns='product_code',  # Use Procedure Code as in your screenshot\n",
    "    values='npi_id',         # Counting unique HCPs\n",
    "    aggfunc=pd.Series.nunique,\n",
    "    fill_value=0\n",
    ")[procedure_groups.keys()]  # Limit to 4 products only\n",
    "\n",
    "# Step 5: Show result\n",
    "new_writers"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 226,
   "id": "a193e216-5ae8-43f3-a86f-29a0ad699862",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 1000x600 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "# New HCP Writers per Product per Year (2016–2018) OR First-Time Prescribers by Product and Year\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "\n",
    "# Step 1: Ensure correct year column\n",
    "main_data['Year'] = pd.to_datetime(main_data['claim_date']).dt.year\n",
    "\n",
    "# Step 2: Filter for 2016–2018 and relevant 4 anesthesia products\n",
    "procedure_groups = {\n",
    "    'J1885': 'Ketotrom',\n",
    "    'J2250': 'Midoride',\n",
    "    'J3010': 'Fentirate',\n",
    "    'J2704': 'Profativ'\n",
    "}\n",
    "filtered_df = main_data[\n",
    "    (main_data['Year'].isin([2016, 2017, 2018])) &\n",
    "    (main_data['product_code'].isin(procedure_groups.keys()))\n",
    "].copy()\n",
    "\n",
    "# Step 3: Identify year of first prescription per HCP and product\n",
    "first_prescription = (\n",
    "    filtered_df.groupby(['npi_id', 'product_code'])['Year']\n",
    "    .min()\n",
    "    .reset_index()\n",
    "    .rename(columns={'Year': 'First_Year_Prescribed'})\n",
    ")\n",
    "\n",
    "# Step 4: Merge to identify new writer records\n",
    "merged_df = filtered_df.merge(first_prescription, on=['npi_id', 'product_code'])\n",
    "new_writers_df = merged_df[merged_df['Year'] == merged_df['First_Year_Prescribed']]\n",
    "\n",
    "# Step 5: Count new writers by year and product\n",
    "new_writer_counts = (\n",
    "    new_writers_df.groupby(['Year', 'product_code'])['npi_id']\n",
    "    .nunique()\n",
    "    .reset_index(name='New_Writer_Count')\n",
    ")\n",
    "\n",
    "# Step 6: Map product codes to names for clean display\n",
    "new_writer_counts['Product'] = new_writer_counts['product_code'].map(procedure_groups)\n",
    "\n",
    "# Step 7: Set product display order and colors\n",
    "product_order = ['Fentirate', 'Ketotrom', 'Midoride', 'Profativ']\n",
    "product_colors = {\n",
    "    'Fentirate': '#1f77b4',  # Blue\n",
    "    'Ketotrom': '#ff7f0e',   # Orange\n",
    "    'Midoride': '#2ca02c',   # Green\n",
    "    'Profativ': '#d62728'    # Red\n",
    "}\n",
    "\n",
    "# Step 8: Plot\n",
    "plt.figure(figsize=(10, 6))\n",
    "sns.barplot(\n",
    "    data=new_writer_counts,\n",
    "    x='Year',\n",
    "    y='New_Writer_Count',\n",
    "    hue='Product',\n",
    "    order=[2016, 2017, 2018],\n",
    "    hue_order=product_order,\n",
    "    palette=product_colors\n",
    ")\n",
    "\n",
    "# Add bar labels\n",
    "for container in plt.gca().containers:\n",
    "    plt.bar_label(container, fmt='%d', padding=3, fontsize=10)\n",
    "\n",
    "# Formatting\n",
    "plt.title('New HCP Writers per Product per Year (2016–2018)', fontsize=16, pad=20)\n",
    "plt.xlabel('Claim Year', fontsize=14)\n",
    "plt.ylabel('Number of New Writers', fontsize=14)\n",
    "plt.legend(title='Product', loc='upper right')\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
  },
