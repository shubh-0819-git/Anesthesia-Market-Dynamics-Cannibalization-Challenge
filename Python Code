    "# Filter the data for years 2016 to 2018\n",
    "main_data['claim_date'] = pd.to_datetime(main_data['claim_date'])\n",
    "data_filtered = main_data[main_data['claim_date'].dt.year.isin([2016, 2017, 2018])]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 167,
   "id": "f1aa2c7f-9996-4070-b2db-eb03b0a1c330",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
