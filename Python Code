    "# Step 1: Ensure 'claim_date' is datetime and extract year\n",
    "joined_df_2016_2018['Claim_year'] = pd.to_datetime(joined_df_2016_2018['claim_date']).dt.year\n",
    "\n",
    "# Step 2: Define relevant procedure codes\n",
    "procedure_groups = {\n",
    "    'J1885': 'Ketotrom',\n",
    "    'J2250': 'Midoride',\n",
    "    'J3010': 'Fentirate',\n",
    "    'J2704': 'Profativ'\n",
    "}\n",
    "\n",
    "# Step 3: Create pivot table to count unique HCPs (writers)\n",
    "total_writers = joined_df_2016_2018.pivot_table(\n",
    "    index='Claim_year',\n",
    "    columns='product_code',  # <- use 'product_code' if it contains 'J1885', etc.\n",
    "    values='npi_id',         # <- use correct HCP ID column\n",
    "    aggfunc=pd.Series.nunique,\n",
    "    fill_value=0\n",
    ")[procedure_groups.keys()]  # limit to your 4 target procedure codes\n",
    "\n",
    "# Step 4: Display result\n",
    "total_writers"
   ]
  },
