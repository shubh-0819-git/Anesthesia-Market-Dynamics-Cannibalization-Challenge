   "source": [
    "# calculate year-over-year (YoY) change (%) in claims volume for Midoride (J2250) between 2017 and 2018 by territory:\n",
    "\n",
    "# Filter for Midoride (J2250) and add year column\n",
    "variant_brand_claims = main_data[\n",
    "    (main_data['product_code'] == 'J2250') &\n",
    "    (pd.to_datetime(main_data['claim_date']).dt.year.isin([2016, 2017, 2018]))\n",
    "].copy()\n",
    "variant_brand_claims['Claim_year'] = pd.to_datetime(variant_brand_claims['claim_date']).dt.year\n",
    "\n",
    "# Create pivot table: rows = Territory, columns = Year, values = claim counts\n",
    "variant_brand_territory_level = variant_brand_claims.pivot_table(\n",
    "    index='Claim_year',\n",
    "    columns='Territory Name',\n",
    "    aggfunc='size',\n",
    "    fill_value=0\n",
    ").transpose()\n",
    "\n",
    "# Calculate YoY % change from 2017 to 2018\n",
    "variant_brand_territory_level['yoy_change'] = (\n",
    "    (variant_brand_territory_level[2018] - variant_brand_territory_level[2017]) / \n",
    "     variant_brand_territory_level[2017]\n",
    ") * 100\n",
    "\n",
    "# Display result\n",
    "variant_brand_territory_level"
   ]
  },
