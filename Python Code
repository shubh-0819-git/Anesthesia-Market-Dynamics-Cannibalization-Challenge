    "# Step 1: Define the procedure-to-brand mapping\n",
    "procedure_groups = {\n",
    "    'J1885': 'Ketotrom',\n",
    "    'J2250': 'Midoride',\n",
    "    'J3010': 'Fentirate',\n",
