    "data_filtered.loc[:, 'product_name'] = data_filtered['product_code'].map(product_code_to_name)\n",
    "\n",
    "# Group and count unique claims by year and product\n",
    "total_claims_df = data_filtered.pivot_table(\n",
    "    index=data_filtered['claim_date'].dt.year,\n",
    "    columns='product_name',\n",
    "    values='claim_id',\n",
    "    aggfunc='count',\n",
    "    fill_value=0\n",
    ")\n",
    "\n",
    "# Display the table\n",
    "total_claims_df"
   ]
