   "source": [
    "import pandas as pd\n",
    "\n",
    "# Step 1: Define base path\n",
    "base_path = '/Users/nikitavira/Desktop/Stevens/SEMESTER 2/BIA-810 Healthcare Analytics/Finals/Datasets/'\n",
    "\n",
    "# Step 2: Read all 5 Medicare Claims parts\n",
    "med_clm_data_1 = pd.read_csv(base_path + 'Medicare_Claims_data_part_1.csv')\n",
    "med_clm_data_2 = pd.read_csv(base_path + 'Medicare_Claims_data_part_2.csv')\n",
    "med_clm_data_3 = pd.read_csv(base_path + 'Medicare_Claims_data_part_3.csv')\n",
    "med_clm_data_4 = pd.read_csv(base_path + 'Medicare_Claims_data_part_4.csv')\n",
    "med_clm_data_5 = pd.read_csv(base_path + 'Medicare_Claims_data_part_5.csv')\n",
    "\n",
    "# Step 3: Concatenate them into one dataframe\n",
    "merged_df = pd.concat(\n",
    "    [med_clm_data_1, med_clm_data_2, med_clm_data_3, med_clm_data_4, med_clm_data_5],\n",
    "    ignore_index=True\n",
    ")\n",
    "\n",
    "# Step 4: Sort by Claim ID\n",
    "merged_df = merged_df.sort_values(by=['cur_clm_uniq_id'])\n",
    "\n",
    "# Step 5: Reset index\n",
    "merged_df = merged_df.reset_index(drop=True)\n",
    "\n",
    "# Step 6: Print or inspect\n",
    "print(merged_df.head())  # Safer to print only first few rows"
   ]
  },
