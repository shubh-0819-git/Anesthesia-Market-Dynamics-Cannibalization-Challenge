   "cell_type": "markdown",
   "id": "194c01c2-6202-4fc4-970e-b5b9c2138b21",
   "metadata": {},
   "source": [
    "## 1]  A 100% stacked bar graph showing claims percentage of each product per year, where one bar represents one year: ##"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 173,
   "id": "b983a1b3-2a12-42f6-ab97-b3db000285e0",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 1200x800 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Map product codes to names\n",
    "code_map = {\"J1885\": \"Ketotrom\", \"J2250\": \"Midoride\", \"J3010\": \"Fentirate\", \"J2704\": \"Profativ\"}\n",
    "colors = {\"Fentirate\": '#4C72B0', \"Ketotrom\": '#DD8452', \"Midoride\": '#55A868', \"Profativ\": '#C44E52'}\n",
    "\n",
    "# Prepare data\n",
    "main_data['product_name'] = main_data['product_code'].map(code_map)\n",
    "main_data['Year'] = pd.to_datetime(main_data['claim_date']).dt.year\n",
    "df = main_data[main_data['Year'].isin([2016, 2017, 2018])]\n",
    "\n",
    "# Group and calculate percentages\n",
    "grouped = df.groupby(['Year', 'product_name']).size().unstack(fill_value=0)\n",
    "percentages = grouped.div(grouped.sum(axis=1), axis=0) * 100\n",
    "\n",
    "# Plot\n",
    "ax = percentages.plot(kind='bar', stacked=True, figsize=(12, 8), color=[colors[col] for col in percentages.columns])\n",
    "ax.set(title='Claims Percentage of Each Product Per Year', xlabel='Year', ylabel='Percentage of Claims (%)')\n",
    "ax.set_ylim(0, 105)\n",
    "ax.legend(title='Product', bbox_to_anchor=(1.05, 1), loc='upper left')\n",
    "ax.tick_params(labelsize=14)\n",
    "\n",
    "# Add labels\n",
    "for i, year in enumerate(percentages.index):\n",
    "    y = 0\n",
    "    for product in percentages.columns:\n",
    "        pct = percentages.loc[year, product]\n",
    "        if pct > 0.3:\n",
    "            ax.text(i, y + pct / 2, f'{pct:.1f}%', ha='center', va='center',\n",
    "                    fontsize=13, color='white' if pct > 20 else 'black')\n",
    "        y += pct\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 174,
