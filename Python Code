   "source": [
    "## Function for its Zip code to make it into standard 5-digit format ##"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 151,
   "id": "25bb7851-9f2d-433f-a051-0de05ff5a79b",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "  Zip Code Territory Name Region Name\n",
      "0    00501   St Louis, MO     Midwest\n",
      "1    00544   St Louis, MO     Midwest\n",
      "2    00601   St Louis, MO     Midwest\n",
      "3    00602   St Louis, MO     Midwest\n",
      "4    00603   St Louis, MO     Midwest\n"
     ]
    }
   ],
   "source": [
    "# Convert 'Zip Code' column to string type\n",
    "zip_df['Zip Code'] = zip_df['Zip Code'].astype(str)\n",
    "\n",
    "# Function to adjust the zip codes\n",
    "def adjust_zip(zip_code):\n",
    "    if len(zip_code) < 5:\n",
    "        return '0' * (5 - len(zip_code)) + zip_code\n",
    "    else:\n",
    "        return zip_code\n",
    "\n",
    "# Applying the function to the 'Zip Code' column\n",
    "zip_df['Zip Code'] = zip_df['Zip Code'].apply(adjust_zip)\n",
    "\n",
    "# Display the modified DataFrame\n",
    "print(zip_df.head())"
   ]
  },
