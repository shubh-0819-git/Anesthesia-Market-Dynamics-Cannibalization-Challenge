   "source": [
    "# Territory-level claims volume for Product J2250 (Midoride) per year:\n",
    "\n",
    "# Filter for Midoride (J2250) and years 2016-2018\n",
    "variant_brand_claims = main_data[\n",
    "    (main_data['product_code'] == 'J2250') &\n",
    "    (pd.to_datetime(main_data['claim_date']).dt.year.isin([2016, 2017, 2018]))\n",
    "].copy()\n",
    "\n",
    "# Add Claim_year column\n",
    "variant_brand_claims['Claim_year'] = pd.to_datetime(variant_brand_claims['claim_date']).dt.year\n",
    "\n",
    "# Pivot to get territory-level claim counts per year\n",
    "variant_brand_territory_level = variant_brand_claims.pivot_table(\n",
    "    index='Claim_year',\n",
    "    columns='Territory Name',\n",
    "    aggfunc='size',\n",
    "    fill_value=0\n",
    ")\n",
    "\n",
    "# Transpose so each row is a territory\n",
    "variant_brand_territory_level = variant_brand_territory_level.transpose()\n",
    "\n",
    "# Display result\n",
    "variant_brand_territory_level"
   ]
