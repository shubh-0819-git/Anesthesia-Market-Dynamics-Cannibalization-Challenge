    "# Get the total number of unique claims for Product J2250 (Midoride) per territory in 2016–2018\n",
    "variant_brand_claims = main_data[\n",
    "    (main_data['product_code'] == 'J2250') & \n",
    "    (pd.to_datetime(main_data['claim_date']).dt.year.isin([2016, 2017, 2018]))\n",
    "]\n",
    "\n",
    "# Display the filtered DataFrame\n",
    "variant_brand_claims.head()"
