    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Define your custom colors\n",
    "colors = {\n",
    "    'Fentirate': '#4C72B0',\n",
    "    'Ketotrom': '#DD8452',\n",
    "    'Midoride': '#55A868',\n",
    "    'Profativ': '#C44E52'\n",
    "}\n",
    "\n",
    "# Plot only the bottom chart (patients per writer)\n",
    "plt.figure(figsize=(10, 6))\n",
    "for product in patients_per_hcp_unstacked.columns:\n",
    "    plt.plot(patients_per_hcp_unstacked.index, \n",
    "             patients_per_hcp_unstacked[product], \n",
    "             marker='o', \n",
    "             label=product, \n",
    "             color=colors[product])\n",
    "\n",
    "plt.title('Number of Patients per Writer per Year', fontsize=16)\n",
    "plt.xlabel('Year', fontsize=14)\n",
    "plt.ylabel('Patients per Writer', fontsize=14)\n",
    "plt.xticks([2016, 2017, 2018], fontsize=10)\n",
    "plt.yticks(fontsize=10)\n",
    "plt.legend(title='Products', bbox_to_anchor=(1, 1), loc='upper left')\n",
    "plt.grid(True)\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 194,
   "id": "ff262711-5997-4db1-b7cf-1818e50648ba",
