   "source": [
    "# Extract claim year from the date\n",
    "data_filtered.loc[:, 'year'] = data_filtered['claim_date'].dt.year\n",
    "\n",
    "# Calculate average number of claims per HCP for each product per year\n",
    "claims_per_hcp = (\n",
    "    data_filtered.groupby(['product_name', 'year'])['claim_id'].count() /\n",
    "    data_filtered.groupby(['product_name', 'year'])['npi_id'].nunique()\n",
    ")\n",
    "\n",
    "# Unstack for plotting\n",
    "claims_per_hcp_unstacked = claims_per_hcp.unstack(level=0)[['Ketotrom', 'Midoride', 'Fentirate', 'Profativ']]\n",
    "\n",
    "# Display the result\n",
    "claims_per_hcp_unstacked"
   ]
