   "source": [
    "# Filter for competitor product J3010 (Fentirate) and years 2016-2018\n",
    "competitor_brand_claims = main_data[\n",
    "    (main_data['product_code'] == 'J3010') &\n",
    "    (main_data['claim_date'].dt.year.isin([2016, 2017, 2018]))\n",
    "]\n",
    "\n",
    "# Extract claim year\n",
    "competitor_brand_claims = competitor_brand_claims.copy()\n",
    "competitor_brand_claims['Claim_year'] = competitor_brand_claims['claim_date'].dt.year\n",
    "\n",
    "# Create pivot table: number of claims by territory and year\n",
    "competitor_brand_territory_level = competitor_brand_claims.pivot_table(\n",
    "    index='Claim_year',\n",
    "    columns='Territory Name',\n",
    "    aggfunc='size',\n",
    "    fill_value=0\n",
    ")\n",
    "\n",
    "# Display the result\n",
    "competitor_brand_territory_level"
   ]
  },
