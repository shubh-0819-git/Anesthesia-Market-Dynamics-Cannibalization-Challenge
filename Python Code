   "cell_type": "code",
   "execution_count": 224,
   "id": "ab1397b3-650d-4ddf-9035-ee8332452ece",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "    Claim_year Product_brand  New_Writer_Count\n",
      "0         2016     Fentirate               102\n",
      "1         2016      Ketotrom               218\n",
      "2         2016      Midoride               151\n",
      "3         2016      Profativ                46\n",
      "4         2017     Fentirate               170\n",
      "5         2017      Ketotrom               182\n",
      "6         2017      Midoride               172\n",
      "7         2017      Profativ               125\n",
      "8         2018     Fentirate               223\n",
      "9         2018      Ketotrom                99\n",
      "10        2018      Midoride               107\n",
      "11        2018      Profativ               193\n"
     ]
    }
   ],
   "source": [
    "import pandas as pd\n",
    "\n",
    "# Ensure datetime is correct\n",
    "main_data['claim_date'] = pd.to_datetime(main_data['claim_date'])\n",
    "\n",
    "# Extract claim year\n",
    "main_data['Claim_year'] = main_data['claim_date'].dt.year\n",
    "\n",
    "# Define mapping for procedure codes to product brands\n",
    "procedure_groups = {\n",
    "    'J1885': 'Ketotrom',\n",
    "    'J2250': 'Midoride',\n",
    "    'J3010': 'Fentirate',\n",
    "    'J2704': 'Profativ'\n",
    "}\n",
    "main_data['Product_brand'] = main_data['product_code'].map(procedure_groups)\n",
    "\n",
    "# Find new writers: Drop duplicates so each HCP only appears once per brand\n",
    "new_writers_df = main_data.drop_duplicates(subset=['npi_id', 'Product_brand'])\n",
    "\n",
    "# Count new writers per brand per year\n",
    "new_writer_counts = new_writers_df.groupby(['Claim_year', 'Product_brand'])['npi_id'].nunique().reset_index()\n",
    "new_writer_counts.rename(columns={'npi_id': 'New_Writer_Count'}, inplace=True)\n",
    "\n",
    "# Display the result\n",
    "print(new_writer_counts)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 225,
