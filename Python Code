   "source": [
    "# Convert to percentage\n",
    "percent_df = unique_patients_df.div(unique_patients_df.sum(axis=1), axis=0) * 100\n",
    "\n",
    "# Plot\n",
    "ax = percent_df.plot(kind='bar', stacked=True, figsize=(10, 6),\n",
    "                     color=[colors[c] for c in percent_df.columns])\n",
    "\n",
    "# Add labels inside bars\n",
    "for c in ax.containers:\n",
    "    for bar in c:\n",
    "        h = bar.get_height()\n",
    "        ax.text(bar.get_x() + bar.get_width()/2, bar.get_y() + h/2.5,\n",
    "                f'{h:.1f}%', ha='center', va='center', color='white', fontsize=11)\n",
    "\n",
    "# Format\n",
    "plt.title('Share of Patients by Product (2016–2018)', fontsize=16)\n",
    "plt.xlabel('Year')\n",
    "plt.ylabel('Percentage of Patients (%)')\n",
    "plt.legend(title='Product', bbox_to_anchor=(1.05, 1), loc='upper left')\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
