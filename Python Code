   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 1000x600 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Step 1: Extract year from claim date\n",
    "main_data['Year'] = pd.to_datetime(main_data['claim_date']).dt.year\n",
    "\n",
    "# Step 2: Filter for relevant years\n",
    "filtered_data = main_data[main_data['Year'].isin([2016, 2017, 2018])].copy()\n",
    "\n",
    "# Step 3: Define mapping of product codes to brand names\n",
    "procedure_groups = {\n",
    "    'J1885': 'Ketotrom',\n",
    "    'J2250': 'Midoride',\n",
    "    'J3010': 'Fentirate',\n",
    "    'J2704': 'Profativ'\n",
    "}\n",
    "filtered_data['Product'] = filtered_data['product_code'].map(procedure_groups)\n",
    "\n",
    "# Step 4: Drop any rows not in our 4 drugs\n",
    "filtered_data = filtered_data[filtered_data['Product'].notnull()]\n",
    "\n",
    "# Step 5: Total writers = unique HCPs per year and product\n",
    "total_writers = (\n",
    "    filtered_data.groupby(['Year', 'Product'])['npi_id']\n",
    "    .nunique()\n",
    "    .reset_index(name='Total_Writers')\n",
    ")\n",
    "\n",
    "# Step 6: Identify new writers by first prescription year\n",
    "first_year = (\n",
    "    filtered_data.groupby(['npi_id', 'Product'])['Year']\n",
    "    .min()\n",
    "    .reset_index()\n",
    "    .rename(columns={'Year': 'First_Year'})\n",
    ")\n",
    "merged = filtered_data.merge(first_year, on=['npi_id', 'Product'])\n",
    "new_writers = merged[merged['Year'] == merged['First_Year']]\n",
    "new_writer_counts = (\n",
    "    new_writers.groupby(['Year', 'Product'])['npi_id']\n",
    "    .nunique()\n",
    "    .reset_index(name='New_Writers')\n",
    ")\n",
    "\n",
    "# Step 7: Merge and calculate continuing writers\n",
    "writer_summary = pd.merge(total_writers, new_writer_counts, on=['Year', 'Product'], how='left').fillna(0)\n",
    "writer_summary['Continuing_Writers'] = writer_summary['Total_Writers'] - writer_summary['New_Writers']\n",
    "\n",
    "# Step 8: Filter for only 2017 and 2018 for final plot\n",
    "plot_data = writer_summary[writer_summary['Year'].isin([2017, 2018])]\n",
    "\n",
    "# Step 9: Plot the line chart\n",
    "plt.figure(figsize=(10, 6))\n",
    "for product in plot_data['Product'].unique():\n",
    "    subset = plot_data[plot_data['Product'] == product]\n",
    "    plt.plot(subset['Year'], subset['Continuing_Writers'], marker='o', label=product)\n",
    "\n",
    "# Add labels and formatting\n",
    "plt.title('Continuing HCP Writers by Product (2017–2018)', fontsize=16)\n",
    "plt.xlabel('Year', fontsize=14)\n",
    "plt.ylabel('Number of Continuing Writers', fontsize=14)\n",
    "plt.xticks([2017, 2018])\n",
    "plt.grid(axis='y', linestyle='--', alpha=0.5)\n",
    "plt.legend(title='Product', bbox_to_anchor=(1.02, 1), loc='upper left')\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
  },
